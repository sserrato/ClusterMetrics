<div class="container">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>ClusterMetric ID</th>
        <th>Domain</th>
        <th>Bridge</th>
        <th>Month</th>
        <th>Frequency</th>
      </tr>
      <tbody>
        <% @emails.each do |email| %>
            <tr>
              <td><%= email.id%> </td>
              <td><%= email.emaildomain%> </td>
              <td><%= email.bridge%> </td>
              <td><%= email.month%> </td>
              <td><%= email.emailFrequency%> </td>
              <% end %>
            </tr>
        </tbody>
  </table>
</div>
